+TEXT runtime·raise(SB),NOSPLIT,$8
+	MOVD	$8(RSP), R0	// arg 1 &8(RSP)
+	MOVD	$SYS_thr_self, R8
+	SVC
+	MOVD	8(RSP), R0	// arg 1 pid
+	MOVW	sig+0(FP), R1	// arg 2 sig
+	MOVD	$SYS_thr_kill, R8
+	SVC
+	RET
