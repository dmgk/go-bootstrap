+TEXT runtime·raiseproc(SB),NOSPLIT|NOFRAME,$0
+	MOVD	$SYS_getpid, R8
+	SVC
+	 			// arg 1 pid (from getpid)
+	MOVW	sig+0(FP), R1	// arg 2 - sig
+	MOVD	$SYS_kill, R8
+	SVC
+	RET
